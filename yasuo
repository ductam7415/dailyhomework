package halo

data class yasuo(var blood : Int ,var heal: Int, var damage: Int, var armor: Int, var speed: Int, var resistance: Int){
}
enum class Skills {
    Q,
    W,
    E,
    R
}

fun main(args: Array<String>){
    var yasuo = yasuo(blood = 610, damage = 66, speed = 345, heal = 7, armor = 33, resistance = 31)
    println(yasuo.toString())
    println("nhan ${Skills.R} de su dung Trăn Trối")
}
